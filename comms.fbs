namespace rlbot.flat;

table MatchComm {
  /// The index of the player that sent the quick chat
  index:uint;
  /// The team of the player that sent the message
  team:uint;
  /// True if the chat is team only false if everyone can see it.
  team_only:bool;
  /// The message that will be displayed on the screen
  display:String;
  /// The contents of the message
  action:[byte] (required);
}

root_type MatchComm;
